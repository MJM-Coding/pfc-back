{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "maxDuration": 10,
    "maxMemory": 1024,
    "limits": {
      "maxRequestSize": "10mb"
    }
  }
}
