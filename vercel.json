{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "functions": {
    "limits": {
      "maxDuration": 10,
      "maxMemory": 1024
    },
    "maxRequestSize": "10mb" 
  },
  "public": true,
  "outputDirectory": "public"
}
